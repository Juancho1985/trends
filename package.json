{
  "name": "SpaceTraveller",
  "version": "0.0.1",
  "description": "Sample of physics and pixijs.",
  "repository": {
    "type": "git",
    "url": ""
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "underscore"
      ]
    },
    "marionette": {
      "exports": "Marionette",
      "depends": [
        "backbone"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~2.2.2",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.2",
    "del": "~1.1.1",
    "doxx": "^1.2.6",
    "expect.js": "^0.3.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-changed": "^1.1.1",
    "gulp-filesize": "0.0.6",
    "gulp-help": "^1.3.4",
    "gulp-iconfont": "^1.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "~0.5.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-sass": "~2.0.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-swig": "^0.7.4",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "gulp-streamify": "^0.0.4",
    "handlebars": "^3.0.0",
    "hbsfy": "~2.2.1",
    "istanbul-harmony": "^0.3.1",
    "karma": "^0.12.31",
    "karma-browserify": "^4.0.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-mocha": "^0.1.10",
    "karma-nyan-reporter": "0.0.51",
    "karma-sinon-chai": "^0.3.0",
    "merge-stream": "^0.1.7",
    "mocha": "^2.1.0",
    "pretty-hrtime": "~1.0.0",
    "require-dir": "^0.3.0",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^2.4.0",
    "karma-coveralls": "~1.1.2",
    "coveralls": "~2.11.4"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "backbone.marionette": "~2.4.2",
    "typed.js": "git://github.com/mattboldt/typed.js.git",
    "backbone.wreqr": "^1.3.1",
    "bootstrap-sass": "~3.3.4",
    "lodash": "^3.3.1",
    "q": "^1.4.1",
    "jquery": "~2.1.0",
    "moment": "~2.10.6",
    "jquery-ui": "~1.10.5",
    "browserify": "~11.2.0"
  },
  "scripts": {
    "generate-doc": "./node_modules/doxx/bin/doxx -s src/js -T docs",
    "generate-lcov": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover --hok-run-in-context node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 ./test/**/*.spec.js",
    "generate-report": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js report --root coverage lcov",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  }
}
